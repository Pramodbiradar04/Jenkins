pipeline {
    
    agent any;
    
    stages{
        stage("code"){
            steps{
                echo "Code clone completed"
            }
        }
        stage("Build"){
            steps{
                echo "Image is created"
            }
            
        }
        stage("Test"){
            steps{
                echo "Test is done"
            }
            
        }
        stage("Deploy"){
            steps{
                echo "Deploy completed"
            }
        }
    }
}
    
